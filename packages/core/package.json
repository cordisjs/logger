{
  "name": "undios",
  "description": "Fetch-based axios-style HTTP client",
  "version": "0.1.1",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./adapter": {
      "node": {
        "require": "./lib/adapter/node.cjs",
        "import": "./lib/adapter/node.js"
      },
      "browser": {
        "import": "./lib/adapter/browser.js"
      },
      "types": "./lib/adapter/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordiverse/undios.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/cordiverse/undios/issues"
  },
  "homepage": "https://github.com/cordiverse/undios",
  "keywords": [
    "http",
    "fetch",
    "axios",
    "http",
    "undici",
    "client",
    "request",
    "cordis",
    "plugin"
  ],
  "devDependencies": {
    "cordis": "^3.10.0",
    "undici": "^6.6.2"
  },
  "peerDependencies": {
    "cordis": "^3.10.0"
  },
  "dependencies": {
    "cosmokit": "^1.5.2",
    "file-type": "^16.5.4",
    "ws": "^8.16.0"
  }
}
